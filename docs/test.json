{
  "index_name": "wiki",
  "start_urls": ["https://wiki.seeedstudio.com"],
  "stop_urls": [],
  "sitemap_urls": ["https://wiki.seeedstudio.com/sitemap.xml"],
  "sitemap_alternate_links": true,
  "selectors": {
    "lvl1": "header h1",
    "lvl2": "article h2",
    "lvl3": "article h3",
    "lvl4": "article h4",
    "lvl5": "article h5, article td:first-child",
    "lvl6": "article h6",
    "text": "article p, article li, article td:last-child"
  },

  "strip_chars": " .,;:#",
  "custom_settings": {
    "separatorsToIndex": "_",
    "attributesForFaceting": [
      "language",
      "version",
      "type",
      "docusaurus_tag"
    ],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  }
}
